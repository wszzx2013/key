local function validateKey(key)
    -- 这里应该是你的卡密验证逻辑
    -- 示例: 假设有效的卡密格式为 "INJ-XXXX-XXXX-XXXX"
    
    -- 模拟验证过程
    wait(1) -- 模拟网络延迟
    
    -- 示例验证逻辑
    if string.match(key, "^INJ%-%d%d%d%d%-%d%d%d%d%-%d%d%d%d$") then
        return true
    elseif key == "zzxnbnb123" then -- 测试用卡密
        return true
    else
        return false
    end
end

-- 提交按钮点击事件
SubmitButton.MouseButton1Click:Connect(function()
    local key = KeyBox.Text
    if key == "" then
        StatusLabel.Text = "状态: 请输入卡密!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    StatusLabel.Text = "状态: 验证中..."
    StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 150)
    
    local isValid = validateKey(key)
    
if isValid then
    StatusLabel.Text = "状态: 验证成功! 加载功能..."
    StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
    
    -- 卡密验证通过后执行另一个脚本
    local function loadExternalScript()
        -- 方法1A：使用 loadstring（适用于远程代码）
        local success, err = pcall(function()
            local scriptUrl = "https://github.com/wszzx2013/key/tree/main" -- 替换成你的脚本URL
            local scriptContent = game:HttpGet(scriptUrl) -- 获取远程脚本
            loadstring(scriptContent)() -- 执行脚本
        end)
        
        if not success then
            warn("加载脚本失败:", err)
        end

        -- 方法1B：直接嵌入另一个脚本（适用于本地代码）
        -- loadstring([[
        --     print("这是另一个脚本！")
        --     -- 在这里写你的功能代码...
        -- ]])()
    end

    wait(1) -- 延迟1秒（可选）
    loadExternalScript() -- 执行外部脚本
    MainFrame.Visible = false -- 隐藏卡密UI
else
    StatusLabel.Text = "状态: 卡密无效!"
    StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
end
end)

-- 关闭按钮点击事件
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- 回车键提交
KeyBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        SubmitButton.MouseButton1Click:Wait()
    end
end)
